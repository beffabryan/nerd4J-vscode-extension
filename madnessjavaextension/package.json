{
  "name": "madnessjavaextension",
  "displayName": "MadnessJavaExtension",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Snippets",
    "Programming Languages"
  ],
  "extensionDependencies": [
    "vscjava.vscode-java-pack"
  ],
  "activationEvents": [
    "onContextMenu"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "snippets": [
      {
        "language": "java",
        "path": "./snippets/java.code-snippets"
      }
    ],
    "languages": [
      {
        "id": "java",
        "extensions": [
          ".java"
        ]
      }
    ],
    "commands": [
      {
        "command": "madnessjavaextension.showContextMenu",
        "title": "Generate ..."
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "madnessjavaextension.showContextMenu",
          "when": "resourceLangId == java",
          "keybinding": "alt+shift+g"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.78.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "javaparser": "^2.0.2"
  }
}
